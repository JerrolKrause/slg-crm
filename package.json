{
  "name": "angular-starter",
  "version": "2.2.0",
  "license": "MIT",
  "scripts": {
    "start": "npm run format && ng serve",
    "format": "npm run format-glob -- \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format-glob": "prettier --write",
    "ng": "ng",
    "build": "ng build",
    "prod": "http-server ./dist -o -g",
    "build:prod": "ng build --target=production --environment=prod",
    "deploy": "ng build --prod --base-href /angular-starter/ && angular-cli-ghpages --no-silent",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server",
    "build:client-and-server-bundles": "ng build --prod && ng run angular-starter:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "0.6.3",
    "@angular/animations": "6.1.2",
    "@angular/cdk": "6.4.7",
    "@angular/common": "6.1.2",
    "@angular/compiler": "6.1.1",
    "@angular/core": "6.1.2",
    "@angular/elements": "6.1.2",
    "@angular/forms": "6.1.2",
    "@angular/http": "6.1.2",
    "@angular/platform-browser": "6.1.2",
    "@angular/platform-browser-dynamic": "6.1.2",
    "@angular/platform-server": "6.1.4",
    "@angular/router": "6.1.2",
    "@angular/service-worker": "6.1.2",
    "@ng-bootstrap/ng-bootstrap": "3.0.0",
    "@ngrx/entity": "6.0.1",
    "@ngrx/store": "6.0.1",
    "@nguniversal/express-engine": "6.1.0",
    "@nguniversal/module-map-ngfactory-loader": "6.1.0",
    "@schematics/update": "0.7.4",
    "angular-resizable-element": "3.0.0",
    "bootstrap": "4.1.3",
    "core-js": "2.5.7",
    "document-register-element": "1.11.0",
    "lodash": "4.17.10",
    "ngx-contextmenu": "5.0.3",
    "rxjs": "6.2.2",
    "ts-loader": "4.5.0",
    "typescript-fsa": "2.5.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.6.3",
    "@angular/cli": "6.1.4",
    "@angular/compiler-cli": "6.1.1",
    "@angular/language-service": "6.1.2",
    "@types/jasmine": "2.5.53",
    "@types/jasminewd2": "2.0.2",
    "@types/lodash": "4.14.88",
    "@types/node": "6.0.60",
    "@types/express": "4.16.0",
    "angular-cli-ghpages": "0.5.3",
    "codelyzer": "4.4.4",
    "express": "4.16.3",
    "font-awesome": "4.7.0",
    "jasmine-core": "2.6.2",
    "jasmine-spec-reporter": "4.1.0",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "prettier": "1.14.2",
    "protractor": "5.1.2",
    "ts-node": "3.2.0",
    "tslint": "5.11.0",
    "tslint-angular": "1.1.2",
    "typescript": "2.9.2",
    "webpack-cli": "3.1.0"
  }
}
